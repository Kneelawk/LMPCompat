{
  "schemaVersion": 1,
  "id": "lmp-compat",
  "version": "${version}",

  "name": "LMP Compat",
  "description": "Improves compatibility between LibMultiPart and other mods.",
  "authors": [
    "Kneelawk"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/lmp-compat/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.kneelawk.lmpcompat.LMPCompatMod"
    ]
  },
  "mixins": [
    "lmp-compat.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "libmultipart": ">=0.7.3-pre.3",
    "minecraft": "~1.18.2",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
